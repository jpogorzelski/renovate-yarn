{
  "extends": ["config:base"],
  "timezone": "Europe/Warsaw",
  "rangeStrategy": "pin",
  "separateMultipleMajor": true,
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true,
      "automergeType": "branch",
      "additionalBranchPrefix": "automerge-"
    }
  ]
}
